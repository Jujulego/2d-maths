{
  "name": "@jujulego/2d-maths",
  "version": "1.6.0",
  "license": "MIT",
  "author": "Julien Capellari <julien.capellari@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jujulego/2d-maths"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "scripts": {
    "build": "jill group build:cjs // build:esm // build:types",
    "build:cjs": "swc src -d dist/cjs -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm",
    "build:types": "tsc --project tsconfig.types.json",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "test": "jest"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@jujulego/jill": "2.1.2",
    "@swc/cli": "0.1.61",
    "@swc/core": "1.3.42",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.5.0",
    "@types/node": "16.18.12",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.33.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.5.0",
    "rimraf": "4.1.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
